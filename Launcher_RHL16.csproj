    <ApplicationIcon>icon.ico</ApplicationIcon>
    <Obfuscate>true</Obfuscate>
    <Authors>Exodren</Authors>
    <Company>Exodren Dev</Company>
    <Product>RHL16 Bypass Launcher</Product>
